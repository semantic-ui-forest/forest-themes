.ui.table {
  font-size: 12px;
  a:not(.button) {
    color: #fff;
    text-decoration: underline;
  }

  .dropdown a {
    text-decoration: none;
  }

  > thead > tr > th,
  > tbody > tr > th,
  > tfoot > tr > th,
  > thead > tr > td,
  > tbody > tr > td,
  > tfoot > tr > td {
    border-color: transparent;
  }
}
