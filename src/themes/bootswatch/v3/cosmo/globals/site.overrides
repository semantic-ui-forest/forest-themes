body {
  -webkit-font-smoothing: antialiased;
}

.text.primary,
.text.primary:hover {
  color: @primaryColor;
}

.text.positive,
.text.positive:hover {
  color: @positiveColor;
}

.text.negative,
.text.negative:hover {
  color: @negativeColor;
}

.text.warning,
.text.warning:hover {
  color: @warningColor;
}

.text.info,
.text.info:hover {
  color: @infoColor;
}
