@fontFamily: "Nunito Sans", -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol" !default;
@textColor: @gray-darker;
@h1: 2rem;
@h2: 1.75rem;
@h3: 1.5rem;
@h4: 1.25rem;
@h5: 1rem;
@h6: 0.75rem;
@textTransform: uppercase;
