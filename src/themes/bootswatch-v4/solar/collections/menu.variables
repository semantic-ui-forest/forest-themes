@itemTextColor: @textColor;
@background: @gray-dark;
@border: transparent;
@hoverItemTextColor: rgba(255, 255, 255, 0.75);
@activeItemTextColor: #fff;
@activeItemBackground: darken(@gray-darker, 5%);
@labelTextColor: #fff;
@labelBackground: @gray-dark;
@invertedBackground: darken(@primaryColor, 15%);
@invertedItemTextColor: rgba(255, 255, 255, 0.5);
@invertedHoverColor: rgba(255, 255, 255, 0.75);
@invertedActiveColor: #fff;
@invertedMenuPressedBackground: rgba(255, 255, 255, 0.5);
@hoverItemBackground: transparent;
@diabledTextColor: @gray;
@tabularBorderColor: @gray-dark;
@tabularActiveColor: rgba(255, 255, 255, 0.75);
