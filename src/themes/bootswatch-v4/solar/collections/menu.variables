@tabularActiveBorderColor: @gray-dark;
@paginationActiveTextColor: rgba(255, 255, 255, 0.75);
@paginationActiveBackground: @gray-dark;

@subMenuTextColor: rbga(0, 0, 0, 0.1);
@dropdownBackground: @gray-dark;

@hoverItemBackground: rgba(0, 0, 0, 0.7);
@pressedItemBackground: rgba(0, 0, 0, 0.75);
@activeHoverItemBackground: rgba(0, 0, 0, 0.75);
@verticalItemBackground: @gray-darker;

@secondaryActiveItemBackground: darken(@gray-darker, 5%);
